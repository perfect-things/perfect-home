{
	"manifest_version": 2,
	"name": "Perfect Home",
	"version": "0.14.0",
	"author": "PerfectThings",
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"chrome_settings_overrides": {
		"homepage": "index.html"
	},
	"description": "New tab replacement",
	"permissions": [
		"bookmarks",
		"storage"
	],
	"icons": {
		"48": "icon48.png",
		"96": "icon96.png"
	},
	"browser_specific_settings": {
		"gecko": {
			"id": "page-spotlight@perfectthings.dev",
			"strict_min_version": "80.0"
		}
	}
}
